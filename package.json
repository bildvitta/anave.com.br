    "tailwindcss-aspect-ratio": "^3.0.0",
    "vue-typed-js": "^0.1.2"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^2.1.0",
    "@vue/test-utils": "^1.0.3",
    "ava": "^3.11.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
