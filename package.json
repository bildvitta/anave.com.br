    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "tailwindcss-responsive-embed": "^1.0.0"
  }
